function htmlEscape(str){
    "use strict";
    let result="";
    for(let ch of str){
        if(ch=='<') result+='&lt;';
        else if(ch=='>') result+='&gt;';
        else if(ch=='"') result+='&quot;';
        else if(ch=='\'') result+='&#39;';
        else result+=ch;
    }
    console.log(result);
}
console.log(htmlEscape("<div color='red'> I am a div </div>"))